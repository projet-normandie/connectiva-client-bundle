{
  "name": "projet-normandie/connectiva-client-bundle",
  "description": "Symfony bundle to use connectiva client",
  "keywords": ["connectiva", "pbx", "symfony"],
  "type": "symfony-bundle",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Benard David",
      "email": "magicbart@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2||^8.0",
    "symfony/framework-bundle": "^6.0 || ^7.0",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": { "ProjetNormandie\\ConnectivaBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": {
      "ProjetNormandie\\ConnectivaBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true
    }
  }
}
